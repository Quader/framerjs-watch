# So to hide the layer for a group named "Main Screen" you can do:
# ionuwearablecompanionLayers3["Main Screen"].visible = false

#colors
transparent = 'rgba(0,0,0,0)'
red = 'rgba(255, 0, 0, 1)'

# Background
bgLayer = new Layer
	x: 636
	y: 374
	width: 330
	height: 328
	blur: 2
bgLayer.image = "images/bg.png"
bgBlurLayer = new Layer
	x: 636
	y: 374
	width: 330
	height: 328
bgBlurLayer.image = "images/bg.png"

#Layers
dotsLayer = new Layer
	x: 636
