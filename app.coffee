cablingBtn = new Layer 
	x: 140
	y: 254
	height: 50
	width: 50
	opacity: 1
	backgroundColor: transparent
cablingBtn.html = "<img src='data:image/svg+xml;base64,'>"
uiLayer.addSubLayer(cablingBtn)

settingsBtn = new Layer 
	x: 254
	y: 139
	height: 50
	width: 50
	opacity: 1
	backgroundColor: transparent
settingsBtn.html = "<img src='data:image/svg+xml;base64,'>"
uiLayer.addSubLayer(settingsBtn)

#states
curve1 = "spring(300,20,50)"
bgBlurLayer.states.add 'blurred',
	blur: 3
	brightness: 50
	scale: 0.99
bgBlurLayer.states.animationOptions = 
	curve: 'linear'
	time: 0.1

menuBtnLayer.states.add
	clicked: {scale: 1.2}
	mouseup: {scale: 0.9}

menuBtnLayer.states.animationOptions = 
	curve: 'spring'
	time: 0.1

menuBtnOpenLayer.states.add 'hidden',
	opactiy: 0
	scale: 0
	rotation: -90
	blur: 5

menuBtnOpenLayer.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.2

menuBtnCloseLayer.states.add 'hidden',
	opactiy: 0
	scale: 0
	rotation: 90
	blur: 5

menuBtnCloseLayer.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.2

menuBtnCloseLayer.states.switch "hidden"

alarmsBtn.states.add 'hidden',
	opacity: 0
	blur: 40
	x: -40
alarmsBtn.states.add 'clicked',
	scale: 0.9

alarmsBtn.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.2
alarmsBtn.states.switch 'hidden'

cablingBtn.states.add 'hidden',
	opacity: 0
	blur: 10
	y: 370
cablingBtn.states.add 'clicked',
	scale: 0.9
cablingBtn.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.2
cablingBtn.states.switch 'hidden'

settingsBtn.states.add 'hidden',
	opacity: 0
	blur: 10
	x: 314
settingsBtn.states.add 'clicked',
	scale: 0.9
settingsBtn.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.2
settingsBtn.states.switch 'hidden'

timeLayer.states.add 'hidden',
	opacity: 0
	scale: 0.5
	blur: 10

timeLayer.states.animationOptions = 
	curve: 'bezier-curve'
	time: 0.1

#Events
menuBtnLayer.on Events.TouchStart, ->
	menuBtnLayer.states.switch "mouseup"
	menuBtnOpenLayer.states.next('default', 'hidden')
	menuBtnCloseLayer.states.next('default', 'hidden')
	alarmsBtn.states.next('default', 'hidden')
	cablingBtn.states.next('default', 'hidden')
	settingsBtn.states.next('default', 'hidden')
	bgBlurLayer.states.next('default', 'blurred')
	timeLayer.states.next('default', 'hidden')

	Utils.delay 0.1, -> 
		menuBtnLayer.states.switch "default"

alarmsBtn.on Events.TouchStart, ->
	alarmsBtn.states.switch "clicked"

	Utils.delay 0.1, -> 
		alarmsBtn.states.switch "default"

settingsBtn.on Events.TouchStart, ->
	settingsBtn.states.switch "clicked"

	Utils.delay 0.1, -> 
		settingsBtn.states.switch "default"

cablingBtn.on Events.TouchStart, ->
	cablingBtn.states.switch "clicked"

	Utils.delay 0.1, -> 
		cablingBtn.states.switch "default"

#animations
tickAnimation = secondLayer.animate
	properties: {blur: 4}
	time: 0.2


rotateSec = -> 
	currentRot = secondLayer.rotation

	secondLayer.animate
		properties: {blur: 0, rotation: currentRot + 6}
		time: 0.05
		curve: 'bezier-curve'

	Utils.delay 0.9, ->
		secondLayer.animate
			curve: 'bezier-curve'
			properties: {blur: 0.5, opacity: 0.25}
			time: 0.1

	Utils.delay 0.98, ->
		secondLayer.animate
			curve: 'bezier-curve'
			properties: {blur: 0, opacity: 1}
			time: 0.2

utils.interval(1,rotateSec)

rotateMin = -> minuteLayer.rotation += 6
utils.interval(60,rotateMin)

hourLayer.animate
	properties: {rotation: 360}
	time: 216000
	repeat: 1000